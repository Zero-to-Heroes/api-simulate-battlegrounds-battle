{
	"name": "api-match-stats",
	"version": "1.0.0",
	"description": "",
	"scripts": {
		"lint": "eslint --color --fix --ext .ts .",
		"start": "webpack -w --color",
		"test": "jest",
		"singleTest": "node --max_old_space_size=6000 ./node_modules/jest/bin/jest.js --runInBand -t",
		"build": "npm audit fix && webpack --env.buildEnv=prod --color",
		"package": "sam package --template-file template.yaml --s3-bucket com.zerotoheroes.artifact --output-template-file template.packaged.yaml",
		"deploy": "sam deploy --template-file template.packaged.yaml --stack-name SimulateBgsBattleStack --capabilities CAPABILITY_IAM",
		"clean": "rm -rf -v dist/*"
	},
	"repository": {},
	"author": "",
	"license": "MIT",
	"private": true,
	"dependencies": {
		"@firestone-hs/reference-data": "^0.1.20",
		"aws-sdk": "^2.524.0",
		"jest-extended": "^0.11.5",
		"jest-text-transformer": "^1.0.2",
		"mysql": "^2.17.1",
		"node-fetch": "^2.6.0",
		"serverless-mysql": "^1.5.3"
	},
	"devDependencies": {
		"@types/aws-lambda": "8.10.31",
		"@types/jest": "^25.1.3",
		"@types/mysql": "^2.15.7",
		"@types/node": "12.7.4",
		"@typescript-eslint/eslint-plugin": "2.1.0",
		"@typescript-eslint/parser": "2.1.0",
		"eslint": "^6.3.0",
		"eslint-config-prettier": "^6.2.0",
		"eslint-loader": "^3.0.0",
		"eslint-plugin-prettier": "^3.1.0",
		"jest": "^25.1.0",
		"prettier": "^1.18.2",
		"ts-jest": "^25.2.1",
		"ts-loader": "^6.0.4",
		"ts-node": "^8.3.0",
		"typescript": "^3.6.2",
		"uglifyjs-webpack-plugin": "^2.2.0",
		"webpack": "^4.39.3",
		"webpack-cli": "^3.3.8",
		"yaml-cfn": "^0.2.2"
	},
	"jest": {
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	}
}
